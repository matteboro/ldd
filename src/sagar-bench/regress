#!/bin/tcsh

set BASECMD="./diamond --branch 2 --depth 2 --repeat 2 --vars 3"

foreach I ("" "--oct" "--oct --octCons" "--tvpi" "--tvpi --tvpiCons")
  foreach J ("" "--qelimInt 1" "--summary" "--image" "--summary --preds 2" "--image --preds 2")
    foreach K ("" "--unsat")
      foreach L ("" "--compInv")
        set CMD="$BASECMD $I $J $K $L"
        echo $CMD
        $CMD |& grep ERROR &
      end
    end
    wait
  end
end
